    break\u003cbr /\u003e        return rval\u003cbr /\u003e\u003c/a\u003e\u003cbr /\u003e\u003ca rel\u003d\"nofollow\"\u003e ------------ \u003c/a\u003e\u003cbr /\u003e\u003ca rel\u003d\"nofollow\"\u003e\u003cbr /\u003edef best_hist_sol(arr):\u003cbr /\u003e        lval \u003d leftRange(arr)\u003cbr /\u003e        rval \u003d rightRange(arr)\u003cbr /\u003e        area \u003d [0]*len(arr)\u003cbr /\u003e        for i in range(1,len(arr)):\u003cbr /\u003e                area[i]\u003d(lval[i]+rval[i]+1)*arr[i]\u003cbr /\u003e        return max(area)\u003cbr /\u003e\u003c/a\u003e"},"link":[{"rel":"edit","type":"application/atom+xml","href":"http://www.blogger.com/feeds/6038272452605714757/6194297563868846341/comments/default/4022894469334416067"},{"rel":"self","type":"application/atom+xml","href":"http://www.blogger.com/feeds/6038272452605714757/6194297563868846341/comments/default/4022894469334416067"},{"rel":"alternate","type":"text/html","href":"http://tech-queries.blogspot.com/2011/03/maximum-area-rectangle-in-histogram.html?showComment\u003d1351625393606#c4022894469334416067","title":""}],"author":[{"name":{"$t":"Anonymous"},"email":{"$t":"noreply@blogger.com"},"gd$image":{"rel":"http://schemas.google.com/g/2005#thumbnail","width":"16","height":"16","src":"http://img1.blogblog.com/img/blank.gif"}}],"thr$in-reply-to":{"xmlns$thr":"http://purl.org/syndication/thread/1.0","href":"http://tech-queries.blogspot.com/2011/03/maximum-area-rectangle-in-histogram.html","ref":"tag:blogger.com,1999:blog-6038272452605714757.post-6194297563868846341","source":"http://www.blogger.com/feeds/6038272452605714757/posts/default/6194297563868846341","type":"text/html"},"gd$extendedProperty":[{"name":"blogger.itemClass","value":"pid-771499407"},{"name":"blogger.displayTime","value":"October 31, 2012 at 12:59 AM"}]},{"id":{"$t":"tag:blogger.com,1999:blog-6038272452605714757.post-4463976347896818776"},"published":{"$t":"2012-10-07T12:53:46.440+05:30"},"updated":{"$t":"2012-10-07T12:53:46